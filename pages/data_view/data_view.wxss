.user {
  height: 100rxp;
  width: 80rpx;
  margin: 10rpx;
  position: relative;
  text-align: center;
}

.user text{
  transition: .3s;
}

.user text.isS {
  color: red;
  box-shadow: 10rpx 10rpx 10rpx gray;
  background-color: #fff;
  padding: 4rpx;
  border-radius: 10rpx;
}

.user text.noS {
  color: #3b3b3b;
}

.avatar {
  height: 60rpx;
  width: 60rpx;
  border-radius: 100%;
}

.avatar.noS::after {
  position: absolute;
  top: 0;
  left: 10rpx;
  content: "";
  background-color: rgb(0, 0, 0);
  opacity: 0.5;
  z-index: 1;
  height: 60rpx;
  width: 60rpx;
  border-radius: 100%;
}

.avatar::after {
  position: absolute;
  top: 0;
  left: 10rpx;
  content: "";
  background-color: rgb(0, 0, 0);
  opacity: 0.5;
  z-index: 1;
  height: 60rpx;
  width: 60rpx;
  border-radius: 100%;
}


.avatar.isS::after {
  position: absolute;
  top: 0;
  left: 10rpx;
  content: "";
  background-color: rgb(0, 0, 0);
  opacity: 0;
  z-index: 1;
  height: 60rpx;
  width: 60rpx;
}


.user text {
  font-size: 24rpx;
}

#N5 {
  font-size: 20rpx;
}

.options {
  margin: 20rpx 0;
}

.options>text {
  margin: 30rpx;
  font-size: 28rpx;
}

.comp view {
  display: flex;
}

.confirm {
  width: 120rpx;
  height: 60rpx;
  border-radius: 60rpx;
  background-color: #fff;
  text-align: center;
  line-height: 60rpx;
  position: absolute;
  bottom: 20rpx;
  left: 250rpx;
  border-radius: 50px;
  background: #e0e0e0;
  box-shadow: inset 6px 6px 12px #bebebe,
    inset -6px -6px 12px #ffffff;
    
}

.json{
  width: 100vw;
  height: 80vh;
  overflow: scroll;
  padding: 10rpx;
  background-color: #fcf9f8;
  position: absolute;
  top: -100vh;
  right: -100vw;
  transition: 0.5s;
  opacity: 0;
  padding: 10rpx;
  border-radius: 10rpx;
  box-shadow: 7px 7px 13px #b9b9bc,
    -7px -7px 13px #ffffff;
}

.json.showjson{
  width: 94vw;
  position: absolute;
  background-color: #facbb9;
  top: 100rpx;
  right: 2vw;
  opacity: 1;
}

.json-title{
  display: block;
  width: 100%;
  text-align: center;
  font-size: 36rpx;
  font-weight: 900;
  font-style: italic;
}

.username{
  font-size: 34rpx;
  font-weight: 900;
}


