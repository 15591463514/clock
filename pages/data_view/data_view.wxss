ec-canvas {
  width: 100%;
  height: 100%;
}


.container {
  margin-top: 100rpx;
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}


.title {
  display: flex;
  justify-content: space-between;
  height: 70rpx;
  width: 100%;
  background-color: #f3f3f8;
  position: absolute;
  top: 0rpx;
  left: 0rpx;
  z-index: 100;
}

.title>view {
  padding: 20rpx;
  text-decoration-line: underline;
}

.title view {
  font-size: 40rpx;
  line-height: 70rpx;
}

.title .title-text {
  height: 70rpx;
  line-height: 70rpx;
  font-size: 40rpx;
  display: flex;
  align-items: center;
  justify-content: stretch;
  text-decoration-line: none;
}

.icon-down {
  width: 40rpx;
  height: 40rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.icon-down text {
  transition: 0.5s;
}

.icon-down.fold>text:nth-child(1) {
  height: 4rpx;
  width: 26rpx;
  border-radius: 10rpx;
  background-color: #666666;
  transform: rotate(45deg);
}

.icon-down.fold>text:nth-child(2) {
  height: 4rpx;
  width: 26rpx;
  border-radius: 10rpx;
  background-color: #666666;
  transform: translate(-8rpx) rotate(-45deg);
}

.icon-down.unfold>text:nth-child(1) {
  height: 4rpx;
  width: 26rpx;
  border-radius: 10rpx;
  background-color: #666666;
  transform: rotate(-45deg);
}

.icon-down.unfold>text:nth-child(2) {
  height: 4rpx;
  width: 26rpx;
  border-radius: 10rpx;
  background-color: #666666;
  transform: translate(-8rpx) rotate(45deg);
}


.float {
  width: 200rpx;
  margin: 0 auto;
  box-shadow: 7px 7px 13px #b9b9bc,
    -7px -7px 13px #ffffff;
  border-radius: 0 0 10rpx 10rpx;
  margin-top: -20rpx;
  transition: all 0.5s linear;

}

.float.fold {
  margin-top: -300rpx;
  box-shadow: none;
}

.float.unfold {
  margin-top: 80rpx;
}

.unfold view {
  height: 30rpx;
}

.float view {
  position: relative;
  width: 200rpx;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
  border-top: 1px solid rgb(235, 235, 235);
  box-sizing: border-box;
  transition: all 0.5s linear;
}

.float view:first-child {
  border-top: none;
}


.Echarts.no {
  display: none;
}

.Echarts.yes {
  display: block;
}


.switch {
  position: relative;
  height: 80vh;
  width: 80vw;
  background-color: #2b4bff4b;
  margin: 160rpx auto;
  transition: 0.5s;
  padding: 10rpx;
  border-radius: 10rpx;
  box-shadow: 7px 7px 13px #b9b9bc,
    -7px -7px 13px #ffffff;
}

.switch-title {
  width: 100%;
  text-align: center;
  margin: 10rpx;
  font-size: 36rpx;
}

.switch.close {
  opacity: 0;
  transform: scale(0.5,0.5);
}

.switch.open {
  opacity: 1;
  transform: scale(1,1);
}

.switch>.options {
  width: 100%;
}

